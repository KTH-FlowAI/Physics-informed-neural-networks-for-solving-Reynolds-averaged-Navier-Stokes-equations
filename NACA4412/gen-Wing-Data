# ! /bin/bash

### Reconnecting data to avoid downloading issues
### @yuningw 2024.11.21

FOLDER="./data"
TARGET_FILE="top2n_HighRes.npz"
echo "--------------------------------------"
echo " RECONECT DATA: ${FOLDER}/${TARGET_FILE}"
echo "---------------------------------------"

cat ${FOLDER}/x* > ${FOLDER}/${TARGET_FILE}

echo "---------------------------------------"
echo " COMPLETE: ${FOLDER}/${TARGET_FILE}    "
echo "---------------------------------------"
